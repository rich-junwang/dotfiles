        "vscode-kubernetes.minikube-path.mac": "/Users/juwanga/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
        "vscode-kubernetes.helm-path.mac": "/Users/juwanga/.vs-kubernetes/tools/helm/darwin-amd64/helm"
